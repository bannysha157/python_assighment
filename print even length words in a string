def printevenwords(s):
    s=s.split(' ')
    for word in s:
        if len(word)%2 == 0:
            print(word)
string ="it was nice meeting you"
printevenwords(string)
#o/p
it 
nice


#print odd words

def printevenwords(s):
    s=s.split(' ')
    for word in s:
        if len(word)%2 == 1:
            print(word)
string ="it was nice meeting you"
printevenwords(string)

#o/p
was
metting 
you
